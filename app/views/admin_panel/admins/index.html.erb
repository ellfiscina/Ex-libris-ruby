<br>
<div class="panel panel-default">
  <div class="panel-heading">
    Admins
    <%= link_to new_admin_panel_admin_path, class:"btn btn-success btn-circle pull-right", style:"margin-top: -5px" do %>
      <i class="fa fa-plus"></i>
    <% end %>
  </div>
  <!-- /.panel-heading -->
  <div class="panel-body">
    <div class="table-responsive">
      <table class="table table-striped table-bordered table-hover">
        <thead>
          <tr>
            <th>#</th>
            <th>Email</th>
            <th></th>
          </tr>
        </thead>
        <tbody>
          <% @admins.each do |admin| %>
          <tr>
            <td><%= admin.id %></td>
            <td><%= admin.email %></td>
            <td style="width: 90px">
              <%= link_to edit_admin_panel_admin_path(admin), class: "btn btn-primary btn-circle" do %>
              <i class="fa fa-edit"></i>
              <% end %>
            </td>
          </tr>
          <% end %>
        </tbody>
      </table>
    </div>
    <!-- /.table-responsive -->
  </div>
  <!-- /.panel-body -->
</div>